{
  "name": "admin-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "typeorm": "typeorm",
    "db:gen": "ts-node -P tsconfig.typeorm.json ./node_modules/typeorm/cli migration:generate src/server/db/migrations/auto -d src/server/db/datasource.cli.ts",
    "db:migrate": "ts-node -P tsconfig.typeorm.json ./node_modules/typeorm/cli migration:run -d src/server/db/datasource.cli.ts",
    "db:revert": "ts-node -P tsconfig.typeorm.json ./node_modules/typeorm/cli migration:revert -d src/server/db/datasource.cli.ts",
    "db:seed": "ts-node -P tsconfig.typeorm.json src/server/db/seed.ts",
    "migration:generate": "ts-node -P tsconfig.typeorm.json ./node_modules/typeorm/cli migration:generate -d src/server/db/datasource.cli.ts"
  },
  "dependencies": {
    "@ant-design/nextjs-registry": "^1.2.0",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@google/generative-ai": "^0.24.1",
    "@next-auth/typeorm-legacy-adapter": "^2.0.2",
    "antd": "^5.27.6",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.18",
    "ioredis": "^5.8.2",
    "lucide-react": "^0.548.0",
    "next": "16.0.7",
    "next-auth": "^4.24.13",
    "next-intl": "^4.4.0",
    "pg": "^8.16.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.27",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "baseline-browser-mapping": "^2.8.32",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
